# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  set Page_0 [ipgui::add_page $IPINST -name "Page 0"]
  #Adding Group
  set Memory [ipgui::add_group $IPINST -name "Memory" -parent ${Page_0}]
  set READ_ONLY [ipgui::add_param $IPINST -name "READ_ONLY" -parent ${Memory}]
  set_property tooltip {Make this a ROM instead of a RAM} ${READ_ONLY}
  set C_S00_AXI_DATA_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_DATA_WIDTH" -parent ${Memory} -widget comboBox]
  set_property tooltip {Width of your RAM/ROM word, in bits.} ${C_S00_AXI_DATA_WIDTH}
  set C_S00_AXI_ADDR_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_ADDR_WIDTH" -parent ${Memory} -widget comboBox]
  set_property tooltip {Depth of your RAM/ROM.} ${C_S00_AXI_ADDR_WIDTH}
  set MEMORY_FILE [ipgui::add_param $IPINST -name "MEMORY_FILE" -parent ${Memory}]
  set_property tooltip {The memory initialization file to use.} ${MEMORY_FILE}

  #Adding Group
  set AXI_interface [ipgui::add_group $IPINST -name "AXI interface" -parent ${Page_0}]
  set C_S00_AXI_ID_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_ID_WIDTH" -parent ${AXI_interface}]
  set_property tooltip {Width of ID for write address, write data, read address and read data AXI channels.} ${C_S00_AXI_ID_WIDTH}
  set C_S00_AXI_AWUSER_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_AWUSER_WIDTH" -parent ${AXI_interface}]
  set_property tooltip {The width of the write address user signal} ${C_S00_AXI_AWUSER_WIDTH}
  set C_S00_AXI_ARUSER_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_ARUSER_WIDTH" -parent ${AXI_interface}]
  set_property tooltip {The width of the read address user signal} ${C_S00_AXI_ARUSER_WIDTH}
  set C_S00_AXI_WUSER_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_WUSER_WIDTH" -parent ${AXI_interface}]
  set_property tooltip {The width of the write user signal} ${C_S00_AXI_WUSER_WIDTH}
  set C_S00_AXI_BUSER_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_BUSER_WIDTH" -parent ${AXI_interface}]
  set_property tooltip {The width of the write response user signal} ${C_S00_AXI_BUSER_WIDTH}
  set C_S00_AXI_RUSER_WIDTH [ipgui::add_param $IPINST -name "C_S00_AXI_RUSER_WIDTH" -parent ${AXI_interface}]
  set_property tooltip {The width of the read user signal} ${C_S00_AXI_RUSER_WIDTH}



}

